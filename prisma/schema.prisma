generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL") 
}

model Customer {
  id              String  @id @map("Codigo")

  Loja            String?
  name            String? @map("Nome")
  Fisica_Jurid    String? @map("Fisica/Jurid")
  address         String? @map("Endereco")
  fantasyName     String? @map("N Fantasia")
  neighborhood    String? @map("Bairro")
  Tipo            String?
  state           String? @map("Estado")
  Cd_Municipio    String? @map("Cd.Municipio")
  zipCode         String? @map("CEP")
  city            String? @map("Municipio")
  Regiao          String?
  Desc_Regi_o     String? @map("Desc.Regi√£o")
  Natureza        String?
  End_Cobranca    String? @map("End.Cobranca")
  Bairro_Cob      String? @map("Bairro Cob")
  DDD             String?
  Pes_Tri_Fav_    String? @map("Pes.Tri.Fav.")
  Mun__Cobr_      String? @map("Mun. Cobr.")
  Uf_de_Cobr_     String? @map("Uf de Cobr.")
  DDI             String?
  End_Recebto     String? @map("End.Recebto")
  deliveryAddress String? @map("End.Entrega")
  phone           String? @map("Telefone")
  FAX             String?
  cnpjCpf         String? @map("CNPJ/CPF")
  contact         String? @map("Contato")

  @@index([cnpjCpf], map: "idx_planilha1_cnpj_cpf")
  @@index([name], map: "idx_planilha1_nome")
  @@index([fantasyName], map: "idx_planilha1_n_fantasia")

  @@map("planilha1")
}